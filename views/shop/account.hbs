<!DOCTYPE html>
<html class="no-js" lang="">


<head>

    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>Nodekart - Account</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">

    <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Montserrat:400,700">
    <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Roboto:300,400,700,400italic,700italic&amp;subset=latin,vietnamese">

    <link rel="stylesheet" href="../../../../front-end/css/bootstrap.css">

    <link rel="stylesheet" href="../../../../front-end/css/plugins.css">

    <link rel="stylesheet" href="../../../../front-end/css/styles.css">

    <script src="../../../../front-end/js/vendor.js"></script>

    <script>
        window.SHOW_LOADING = false;
    </script>


</head>

<body>



    <div id="wrapper" class="main-wrapper ">

    {{> shop/header}}
        <!-- /.awe-menubar-header -->


        <div id="main">

            <div class="main-header background background-image-heading-blog">
                <div class="container">
                    <h1>ACCOUNT</h1>
                </div>
            </div>


            <div id="breadcrumb">
                <div class="container">

                    <ol class="breadcrumb">
                        <li><a href="../../../../">Home</a>
                        </li>
                        <li class="active"><span>Account</span>
                        </li>
                    </ol>

          {{#if success_msg}}
            <div class="alert alert-success">
                                <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                                <i class="fa fa-check-square"></i>
                                <span>{{success_msg}}</span>
            </div>
          {{/if}}

          {{#if error_msg}}
          <div class="alert alert-danger">
                                <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                                <i class="fa fa-exclamation-triangle"></i>
                                <span>{{error_msg}}</span>
            </div>
          {{/if}}

          {{#if error}}
            <div class="alert alert-danger">
                                <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                                <i class="fa fa-exclamation-triangle"></i>
                                <span>{{error}}</span>
            </div>
          {{/if}}

                </div>
            </div>


            <div class="blog-wrapper blog-sidebar">
                <div class="container">
                    <div class="row">
                        <div class="col-md-9 col-md-push-3">


        <div class="col-md-12">
          <!-- Horizontal Form -->
          <div class="box box-info">
            <!-- form start -->
            <form class="form-horizontal">
              <div class="box-body">
                <div class="form-group">
                  <label class="col-sm-2 control-label">First Name</label>

                  <div class="col-sm-10">
                    <input type="text" class="form-control" value="{{user.fname}}" disabled>
                  </div>
                </div>

                <div class="form-group">
                  <label class="col-sm-2 control-label">Middle Name</label>

                  <div class="col-sm-10">
                    <input type="text" class="form-control" value="{{user.mname}}" disabled>
                  </div>
                </div>

                <div class="form-group">
                  <label class="col-sm-2 control-label">Last Name</label>

                  <div class="col-sm-10">
                    <input type="text" class="form-control" value="{{user.lname}}" disabled>
                  </div>
                </div>

                <div class="form-group">
                  <label class="col-sm-2 control-label">Username</label>

                  <div class="col-sm-10">
                    <input type="text" class="form-control" value="{{user.username}}" disabled>
                  </div>
                </div>

                <div class="form-group">
                  <label class="col-sm-2 control-label">Email</label>

                  <div class="col-sm-10">
                    <input type="text" class="form-control"  value="{{user.email}}" disabled>
                  </div>
                </div><br><br>


                <div class="form-group">
                  <label class="col-sm-2 control-label">Address Line 1</label>

                  <div class="col-sm-10">
                    <input type="text" class="form-control"  value="{{user.address1}}" disabled>
                  </div>
                </div>

                <div class="form-group">
                  <label class="col-sm-2 control-label">Address Line 2</label>

                  <div class="col-sm-10">
                    <input type="text" class="form-control"  value="{{user.address2}}" disabled>
                  </div>
                </div>

                <div class="form-group">
                  <label class="col-sm-2 control-label">Address Line 3</label>

                  <div class="col-sm-10">
                    <input type="text" class="form-control"  value="{{user.address3}}" disabled>
                  </div>
                </div>

                <div class="form-group">
                  <label class="col-sm-2 control-label">City/Town</label>

                  <div class="col-sm-10">
                    <input type="text" class="form-control"  value="{{user.place}}" disabled>
                  </div>
                </div>

                <div class="form-group">
                  <label class="col-sm-2 control-label">Landmark</label>

                  <div class="col-sm-10">
                    <input type="text" class="form-control"  value="{{user.landmark}}" disabled>
                  </div>
                </div>

                <div class="form-group">
                  <label class="col-sm-2 control-label">District</label>

                  <div class="col-sm-10">
                    <input type="text" class="form-control"  value="{{user.district}}" disabled>
                  </div>
                </div>

                <div class="form-group">
                  <label class="col-sm-2 control-label">State</label>

                  <div class="col-sm-10">
                    <input type="text" class="form-control"  value="{{user.state}}" disabled>
                  </div>
                </div>

                <div class="form-group">
                  <label class="col-sm-2 control-label">Country</label>

                  <div class="col-sm-10">
                    <input type="text" class="form-control"  value="{{user.country}}" disabled>
                  </div>
                </div>

                <div class="form-group">
                  <label class="col-sm-2 control-label">Pincode</label>

                  <div class="col-sm-10">
                    <input type="text" class="form-control"  value="{{user.pin}}" disabled>
                  </div>
                </div>

                <div class="form-group">
                  <label class="col-sm-2 control-label">Phone Number</label>

                  <div class="col-sm-10">
                    <input type="text" class="form-control"  value="{{user.lnumber}}" disabled>
                  </div>
                </div>

                <div class="form-group">
                  <label class="col-sm-2 control-label">Mobile Number</label>

                  <div class="col-sm-10">
                    <input type="text" class="form-control"  value="{{user.mnumber}}" disabled>
                  </div>
                </div>

              </div>
              <br><br>
              <!-- /.box-body -->
              <div class="box-footer">
                <a href="../../../../" class="btn btn-default">Cancel</a>
                <a href="../../../../account/edit" class="btn btn-info pull-right">Edit</a>
              </div>
              <!-- /.box-footer -->
            </form>
          </div>
          <!-- /.box -->
        </div>


               
                        </div>

                        <div class="col-md-3 col-md-pull-9">

                            <div id="widget-area" class="widget-area">

                                <div class="widget widget_categories">
                                    <h3 class="widget-title">Manage Profile</h3>

                                    <ul>
                                        <li class="current"><a href="../../../../account" title="">Account</a>
                                        </li>
                                        <li><a href="../../../../account/change-password" title="">Change Password</a>
                                        </li>
                                        <li><a href="../../../../orders" title="">Orders</a>
                                        </li>
                                        </li>
                                    </ul>
                                </div>


                            </div>

                        </div>
                    </div>
                    <!-- /.row -->
                </div>
                <!-- /.container -->
            </div>
            <!-- /.blog-wrapper -->

        </div>


        {{> shop/footer}}
        <!-- /footer -->

    </div>
    <!-- /#wrapper -->

<script>
$(document).ready(function(){
      setTimeout(function() {
        $('.alert').fadeOut('fast');
      }, 4000); // <-- time in milliseconds
});
</script>


    <script src="http://maps.google.com/maps/api/js?sensor=true"></script>

    <script src="../../../../front-end/js/jquery-ui.min.1.11.4.js"></script>

    <script src="../../../../front-end/js/plugins.js"></script>

    <script src="../../../../front-end/js/main.js"></script>

    <script src="../../../../front-end/js/docs.js"></script>

</body>


</html>
